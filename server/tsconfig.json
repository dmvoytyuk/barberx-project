{
  "compilerOptions": {
    "target": "ES2016",
    // "jsx": "preserve",
    "module": "CommonJS",
    "allowJs": true,
    "baseUrl": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowImportingTsExtensions": true
  },
  "exclude": ["node_modules", "dist", "client"]
}
